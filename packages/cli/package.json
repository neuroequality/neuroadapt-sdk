{
  "name": "@neuroadapt/cli",
  "version": "1.1.0",
  "description": "CLI tool for creating accessible quantum and VR applications",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "neuroadapt": "./dist/cli.js",
    "na": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@neuroadapt/core": "workspace:*",
    "@neuroadapt/quantum": "workspace:*",
    "@neuroadapt/vr": "workspace:*",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.20",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "keywords": [
    "accessibility",
    "cli",
    "quantum",
    "vr",
    "neuroadapt",
    "scaffolding"
  ],
  "author": "NeuroAdapt Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/neuroadapt/neuroadapt-sdk.git",
    "directory": "packages/cli"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}